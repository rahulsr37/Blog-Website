<%- include('partials/header') %>

<h1>Compose</h1>


<form class="" action="/compose" method="post">

    <div class="form-group">
        <label>Title</label>
        <input type="text" class="form-control" name="postTitle" autocomplete="off">
        <label>Post</label>
        <textarea class="form-control" name="postBody" rows="5" cols="155" id="multiLineInput"></textarea>
    </div>
    <button type="submit" class="btn btn-primary" name="button">Publish</button>

</form>



